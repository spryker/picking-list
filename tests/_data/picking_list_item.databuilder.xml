<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="PickingListItem" strict="true">
        <property name="idPickingList" dataBuilderRule="="/>
        <property name="uuid" dataBuilderRule="unique()->uuid()"/>
        <property name="quantity" dataBuilderRule="numberBetween(1, 2)"/>
        <property name="numberOfPicked" dataBuilderRule="=0"/>
        <property name="numberOfNotPicked" dataBuilderRule="=0"/>
    </transfer>
</transfers>
